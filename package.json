{
  "name": "elongoat",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "generate:indexes": "python3 scripts/generate_indexes.py",
    "db:apply-schema": "tsx backend/scripts/apply_schema.ts",
    "db:import:clusters": "tsx backend/scripts/import_clusters.ts",
    "db:import:paa": "tsx backend/scripts/import_paa.ts",
    "db:import:metrics": "tsx backend/scripts/import_keyword_metrics.ts",
    "db:seed:variables": "tsx backend/scripts/seed_variables.ts",
    "db:seed:content": "tsx backend/scripts/seed_content.ts",
    "db:import:all": "npm run db:apply-schema && npm run db:import:clusters && npm run db:import:paa && npm run db:import:metrics",
    "warm:content": "tsx backend/scripts/warm_content.ts",
    "generate:all-clusters": "tsx backend/scripts/generate_all_clusters.ts",
    "generate:questions": "tsx backend/scripts/generate_questions.ts",
    "generate:embeddings": "tsx backend/scripts/generate_embeddings.ts",
    "db:apply-vectors": "psql $DATABASE_URL -f backend/supabase/migrations/001_pgvector.sql",
    "test:generation": "tsx backend/scripts/test_generation.ts",
    "worker:videos": "tsx backend/workers/ingest_videos.ts",
    "worker:x": "tsx backend/workers/ingest_x.ts",
    "warmup": "tsx backend/scripts/startup_warmup.ts",
    "predev": "npm run generate:indexes",
    "prebuild": "npm run generate:indexes",
    "dev": "next dev",
    "build": "next build",
    "build:frontend": "cp next.config.export.mjs next.config.mjs && next build",
    "build:backend": "cp next.config.backend.mjs next.config.mjs && next build",
    "start": "next start",
    "lint": "next lint",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "csv-parse": "6.1.0",
    "dotenv": "17.2.3",
    "framer-motion": "12.23.26",
    "ioredis": "5.8.2",
    "lucide-react": "0.562.0",
    "next": "14.2.35",
    "pg": "8.16.3",
    "react": "^18",
    "react-dom": "^18",
    "react-markdown": "10.1.0",
    "rehype-sanitize": "6.0.0",
    "remark-gfm": "4.0.1",
    "server-only": "0.0.1",
    "zod": "4.2.1"
  },
  "devDependencies": {
    "@tailwindcss/typography": "0.5.19",
    "@types/node": "^20",
    "@types/pg": "8.16.0",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@vitest/coverage-v8": "2.1.9",
    "eslint": "^8",
    "eslint-config-next": "14.2.35",
    "postcss": "^8",
    "tailwindcss": "^3.4.1",
    "tsx": "4.21.0",
    "typescript": "^5",
    "vitest": "2.1.9"
  }
}
